{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.enderman": {
      "geometry": "Geometry.default",
      "part_visibility": [
        { "*": "query.is_invisible ? 0 : 1"}
      ],
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Texture.default" ]
    },
    "controller.render.enderman_invisible": {
      "geometry": "Geometry.invisible",
      "part_visibility": [
        { "head": true }
      ],
      "materials": [ { "*": "Material.invisible" } ],
      "textures": [ "Texture.default" ],
      "ignore_lighting": false,
      "light_color_multiplier": 1.0
    }
  }
}
